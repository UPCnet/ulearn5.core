<?xml version="1.0"?>
<object name="Etherpad" meta_type="Dexterity FTI" i18n:domain="plone"
   xmlns:i18n="http://xml.zope.org/namespaces/i18n">
 <property name="title" i18n:translate="">Etherpad</property>
 <property name="description"
    i18n:translate="">Etherpad collaborative space.</property>
 <property name="icon_expr"></property>
 <property name="factory">Etherpad</property>
 <property name="add_view_expr">string:${folder_url}/++add++Etherpad</property>
 <property name="link_target"></property>
 <property name="immediate_view">view</property>
 <property name="global_allow">False</property>
 <property name="filter_content_types">True</property>
 <property name="allowed_content_types"/>
 <property name="allow_discussion">False</property>
 <property name="default_view">etherpad_view</property>
 <property name="view_methods">
  <element value="pad_view"/>
 </property>
 <property name="default_view_fallback">True</property>
 <property name="add_permission">ulearn.addEtherpad</property>
 <property name="klass">ulearn5.core.content.etherpad.Etherpad</property>
 <property name="behaviors">
  <element value="plone.app.content.interfaces.INameFromTitle" />
  <element value="plone.app.dexterity.behaviors.id.IShortName" />
  <element value="plone.app.dexterity.behaviors.metadata.IDublinCore" />
  <element value="plone.app.relationfield.behavior.IRelatedItems" />
  <element value="collective.dexteritytextindexer.behavior.IDexterityTextIndexer" />
 </property>
 <property name="schema">ulearn5.core.content.etherpad.IEtherpad</property>
 <property name="model_source"></property>
<!--  <property name="model_file">ulearn5.core.schema:pad.xml</property> -->
<!--  <property name="schema_policy">dexterity</property> -->
 <alias from="(Default)" to="etherpad_view"/>
 <alias from="edit" to="@@edit"/>
 <alias from="sharing" to="@@sharing"/>
 <alias from="view" to="(selected layout)"/>
 <action title="View" action_id="view" category="object" condition_expr=""
    description="" icon_expr="" link_target="" url_expr="string:${object_url}/view"
    visible="True">
  <permission value="View"/>
 </action>
 <action title="Edit" action_id="edit" category="object" condition_expr=""
    description="" icon_expr="" link_target=""
    url_expr="string:${object_url}/edit" visible="True">
  <permission value="Modify portal content"/>
 </action>
</object>
