<?xml version="1.0"?>
<object name="portal_controlpanel" meta_type="Plone Control Panel Tool" >
 <configlet title="uLearn settings" action_id="ulearn" appId="ulearn5.core"
    category="Products" condition_expr="python:True"
    icon_expr="string:$portal_url/++resource++cpulearn.jpg"
    url_expr="string:${portal_url}/ulearn-controlpanel" visible="True">
  <permission>Plone Site Setup: uLearn</permission>
 </configlet>
 <configlet title="Ulearn Portlets Settings" action_id="ulearnPortlets"
    appId="ulearn5.core" category="Products" condition_expr=""
    icon_expr="string:${portal_url}/++maxui++static/simbol_MAX_16.png"
    url_expr="string:${portal_url}/@@ulearn-control-portlets" visible="True">
  <permission>Manage portal</permission>
 </configlet>
 <configlet title="uLearn Popup settings" action_id="ulearnPopup"
    appId="ulearn5.core" category="Products" condition_expr=""
    icon_expr="string:${portal_url}/++maxui++static/simbol_MAX_16.png"
    url_expr="string:${portal_url}/@@ulearn-control-popup" visible="True">
  <permission>Plone Site Setup: uLearn</permission>
 </configlet>
</object>
